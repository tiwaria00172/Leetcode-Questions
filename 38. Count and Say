class Solution:
    def countAndSay(self, n):
        if n == 1:
            return "1"
        
        result = "1"
        
        for i in range(2, n + 1):
            prev = result
            result = ""
            count = 1
            
            for j in range(1, len(prev)):
                if prev[j] == prev[j - 1]:
                    count += 1
                else:
                    result += str(count) + prev[j - 1]
                    count = 1
            
            result += str(count) + prev[-1]
        
        return result
